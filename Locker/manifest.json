{
  "name": "LoCKer",
  "version": "2017.0",
  "manifest_version": 99,
  "description": "LoCKer",
  "homepage_url": "https://google.com",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_page": "src/options/index.html",
  "background": {
    "scripts": ["src/inject/inject.js"]
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}